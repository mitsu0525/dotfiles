{
  "title": "ショートカット",
  "rules": [
    {
      "description": "Command + Alt + T でターミナル起動",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": [ "command","option" ]
            }
          },
          "to": [
            { "shell_command": "open -a 'iTerm.app'" }
          ]
        }
      ]
    }
  ]
}

